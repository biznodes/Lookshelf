<div class="contentbottom">
  <div class="contentsep"><img src="images/spacer.gif" alt="" border="0" /></div>
  
  <h2 style="float:left;margin-left:90px;">Here are the books you would like to borrow</h2>

  <%@waiting_list.each do |list|%>   
   <h3 style="float:left;margin-left:90px;"><%=list.lender_name%> has not responded yet.</h3>
   <div class="contentsep"><img src="images/spacer.gif" alt="" border="0" /></div>
    <img src="images/85x63.gif" alt="" class="borrowerImage" />
  <div class="greybox">
    <div class="greybox_con">
      <div class="headingcontent">Book Title:</div><div class="datacontent">test<%=list.book_title%></div>
      <div class="headingcontent">Date of request:</div><div class="datacontent"><%=list.date_of_request%></div>
      <div class="headingcontent">Estimated delivery:</div><div class="datacontent"><%=list.estimated_delivery_date%></div>
      <!--<span class="fontitalic"> Lorem ipsum dolor sit amet, </span> </div>-->
    <button id="submitButton" class="greyButton" onclick="return false;" type="submit">Waiting...</button>
  </div>
  </div>
    <div class="contentsep"><img src="images/spacer.gif" alt="" border="0" /></div>
  <%end%>


 <%@received_list.each do |list|%>
     <h3 style="float:left;margin-left:90px;color:#01DF3A;"><%=list.lender_name%> is willing to loan you this book.</h3>
   <div class="contentsep"><img src="images/spacer.gif" alt="" border="0" /></div>
    <img src="images/85x63.gif" alt="" class="borrowerImage" />
  <div class="greenbox">
    <div class="greenbox_con">
      <div class="headingcontent">Book Title:</div>
      <div class="headingcontent">Date of request:</div>
      <div class="headingcontent">Return date:</div>
    <button id="submitButton" class="greenButton" onclick="return false;" type="submit">Received</button>
  </div>
  </div>
    <div class="contentsep"><img src="images/spacer.gif" alt="" border="0" /></div>
 <%end%>


 <%@loaned_out_list.each do |list|%>
     <h3 style="float:left;margin-left:90px;color:#FD030C;"><%=list.lender_name%> is not willing to loan you this book.</h3>
   <div class="contentsep"><img src="images/spacer.gif" alt="" border="0" /></div>
      <img src="images/85x63.gif" alt="" class="borrowerImage" />
  <div class="redbox">
    <div class="redbox_con">
      <div class="headingcontent">Book Title:</div>
      <div class="headingcontent">Date of request:</div>
      <div class="headingcontent">Return date:</div>
    <button id="submitButton" class="redButton" onclick="return false;" type="submit">Loaned out</button>
  </div>
  </div>
       <div class="contentsep"><img src="images/spacer.gif" alt="" border="0" /></div>
 <%end%>




